<?xml version='1.0' encoding='utf-8'?>
<view name='FoodView' alias='FoodView' xmlns='http://www.lotus.com/dxl' showinmenu='true'
 headers='simple' showresponsehierarchy='true' showmargin='true' shrinkrows='true'
 extendlastcolumn='false' showhierarchies='false' unreadcolor='black' rowlinecount='5'
 headerlinecount='1' rowspacing='1' bgcolor='white' altrowcolor='#f7f7f7'
 totalscolor='black' headerbgcolor='white' boldunreadrows='true' 
 marginwidth='0px' marginbgcolor='white' gridstyle='dot' gridcolor='silver'>
<code event='selection'><formula>SELECT Form="Food"</formula></code>
<actionbar bgcolor='#f0f0f0' bordercolor='black'>
<actionbuttonstyle bgcolor='#f0f0f0'/><font color='system'/><border style='solid'
 width='0px 0px 1px'/>
<action title='_Forward' showinbar='false' systemcommand='forward'/>
</actionbar>
<column sort='ascending' resort='both' hidedetailrows='false' itemname='unid'
 width='10' resizable='true' deferindexcreation='true'>
 <columnheader title='unid'></columnheader>
</column>
<column sort='ascending' resort='both' hidedetailrows='false' itemname='name'
 width='10' resizable='true' deferindexcreation='true'>
 <columnheader title='name'></columnheader>
</column>
<column sort='ascending' resort='both' hidedetailrows='false' itemname='taste'
 width='10' resizable='true' deferindexcreation='true'>
 <columnheader title='taste'></columnheader>
</column>
<column sort='ascending' resort='both' hidedetailrows='false' itemname='color'
 width='10' resizable='true' deferindexcreation='true'>
 <columnheader title='color'></columnheader>
</column>
<column sort='ascending' resort='both' hidedetailrows='false' itemname='shape'
 width='10' resizable='true' deferindexcreation='true'>
 <columnheader title='shape'></columnheader>
</column>
<column sort='ascending' resort='both' hidedetailrows='false' itemname='type'
 width='10' resizable='true' deferindexcreation='true'>
 <columnheader title='type'></columnheader>
</column>
<column itemname='$unid' width='10'>
  <columnheader title='unid'></columnheader>
  <code event='value'>
    <formula>@Text(@DocumentUniqueID)</formula>
  </code>
</column>
</view>
